.card_img {
  background-color: var(--white);
  overflow: hidden;
  border-radius: 30rpx;
}

.card_title {
  display: flex;
  justify-content: center; /* 水平居中 */
  align-items: center; /* 垂直居中 */
  font-weight: bold;
  line-height: 25px;
  font-size: 15px;
  color: black;
  padding-top: 10px;
  padding-bottom: 10px
}

.cu-list.menu>.cu-item:after {
  border-bottom: 0px
}

.iconline text {
  display: inline-block;
  margin-right: 20rpx;
  text-align: center;
}

.info_title {
  color: black;
  font-size: 12px;
  height: 30px;
  line-height: 30px;
  border-bottom: 1px solid rgba(0, 0, 0, .1);
}